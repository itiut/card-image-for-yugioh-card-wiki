{
  "name": "card-image-for-yugioh-card-wiki",
  "description": "Show card images on Yu-Gi-Oh! Card Wiki.",
  "version": "0.2.0",
  "author": "Yuichi Tanikawa",
  "bugs": {
    "url": "https://github.com/itiut/card-image-for-yugioh-card-wiki/issues"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.8",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.14.0",
    "html-loader-jest": "^0.2.1",
    "jest": "^23.5.0",
    "node-fetch": "^2.2.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "homepage": "https://github.com/itiut/card-image-for-yugioh-card-wiki",
  "jest": {
    "transform": {
      "^.+\\.html$": "html-loader-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "license": "MIT",
  "main": "dist/bundle.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/itiut/card-image-for-yugioh-card-wiki"
  },
  "scripts": {
    "build": "webpack",
    "lint": "eslint src test",
    "test": "jest",
    "zip": "npm run build -- -p && zip -r dist.zip dist"
  }
}
