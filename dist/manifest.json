{
  "manifest_version": 2,
  "name": "画像ビューア for 遊戯王カードWiki",
  "version": "0.0.1",
  "description": "遊戯王カードWikiでカード画像を表示・検索できます。",
  "author": "Yuichi Tanikawa",
  "homepage_url": "https://github.com/itiut/image-viewer-for-yugioh-card-wiki",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": ["http://yugioh-wiki.net/index.php?*"],
      "css": ["style.css"],
      "js": ["bundle.js"]
    }
  ],
  "permissions": ["http://yugioh.wikia.com/"],
  "minimum_chrome_version": "42"
}
